{
    "name": "helhum/mono-first-app",
    "repositories": {
        "mono-packages": {
            "url": "../../.mono/",
            "type": "composer"
        },
        "packagist.org": false
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "require": {
        "php": "^8.1",
        "typo3/minimal": "^12"
    }
}
